# Network Configuration
# Set to true to fork from a public testnet
FORK_ENABLED=false

# RPC URL for Sepolia testnet (used when forking)
SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Private key for deploying contracts (if needed)
# WARNING: Never commit your actual private key!
PRIVATE_KEY=

# Gas Reporting
# Set to true to enable gas usage reporting
REPORT_GAS=true
